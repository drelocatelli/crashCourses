	"Print to Console": {
		"prefix": ["sysout", "println"],
		"body": [
			"System.out.println(\"$1\");"
		],
		"description": "Print to the console"
	}

	"Print to Console without ln": {
		"prefix": ["print"],
		"body": [
			"System.out.print(\"$1\");"
		],
		"description": "Print to the console"
	}

	"Public static void main": {
		"prefix": ["main"],
		"body": [
			"public static void main(String[] args){\n\t$1\n}"
		],
		"description": "Main"
	}

}
